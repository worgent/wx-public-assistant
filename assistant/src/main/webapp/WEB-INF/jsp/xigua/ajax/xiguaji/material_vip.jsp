	function loadRecommendArticleList() {
		var bizId = $('a.smartArticleMpSelected').attr('data-id');
		if (bizId == undefined) {
			$.smallBox({
				title: "提示",
				content: "<i class='fa fa-clock-o'></i> <i>请先添加您的公众号</i>",
				color: "#C46A69",
				iconSmall: "fa fa-times fa-2x fadeInRight animated",
				timeout: 3000
			});
			return;
		}
		showLoading();
		var urlParttern = "/UserBiz/List/?partial=1&bizId=" + bizId;
		window.setTimeout(function () {
			$('#recommendArticleList').load(urlParttern, function () {
				hideLoading();
				pageLoad();
				$("[rel=tooltip]").tooltip();
			});
		}, 50);
	}
