				var articleId = $(this).attr('data-articleid');
				var $this = $(this);
				$.getJSON('/MArticleCollect/SetBlack/?articleId=' + articleId, function (result) {
					if (result.Status) {
						$.smallBox({
							title: "不再出现",
							content: "<i class='fa fa-clock-o'></i> <i>成功将该文章设置为不再出现的文章！并为您过滤" + result.FilterCount + "篇跟本文相似的文章。</i>",
							color: "#659265",
							iconSmall: "fa fa-check fa-2x fadeInRight animated",
							timeout: 3000
						});
						$this.parent().parent().css('display', 'none');
					}
					else {
						$.smallBox({
							title: "不再出现",
							content: "<i class='fa fa-clock-o'></i> <i>设置不再出现失败</i>",
							color: "#C46A69",
							iconSmall: "fa fa-times fa-2x fadeInRight animated",
							timeout: 3000
						});
					}
				});
			});
			$html.find('a.articlebaseblack').click(function () {
				var articleId = $(this).attr('data-articleid');
				var $this = $(this);
				$.getJSON('/MArticleCollect/SetBaseBlack/?articleId=' + articleId, function (result) {
					if (result.Status) {
						$.smallBox({
							title: "不再出现",
							content: "<i class='fa fa-clock-o'></i> <i>成功将该文章设置为不再出现的文章！并为您过滤" + result.FilterCount + "篇跟本文相似的文章。</i>",
							color: "#659265",
							iconSmall: "fa fa-check fa-2x fadeInRight animated",
							timeout: 3000
						});
						$this.parent().parent().css('display', 'none');
					}
					else {
						$.smallBox({
							title: "不再出现",
							content: "<i class='fa fa-clock-o'></i> <i>设置不再出现失败</i>",
							color: "#C46A69",
							iconSmall: "fa fa-times fa-2x fadeInRight animated",
							timeout: 3000
						});
					}
				});
			});
		};
		//分组过滤视图里面的按钮过滤事件
		var bindTagArticleView = function () {
			//设置翻页事件
			var p = 1;
			//头条过滤选择
			$('#filterOnlyTopArticle').click(function () {
				p = 1;
				$('#btnLoadMore').html('加载更多...');
				loadArticleList();
			});
			//选择时间
			$('#recentlyDate a').click(function () {
				p = 1;
				$('#btnLoadMore').html('加载更多...');
				$('#recentlyDate a').removeClass('current');
				$(this).addClass('current');
				loadArticleList();
			});
			//列表表头排序
			$('#articleHeader a.orderBy').click(function () {
				p = 1;
				$('#btnLoadMore').html('加载更多...');
				$('#articleHeader a.current').removeClass('current');
				$(this).addClass('current');
				loadArticleList();
			});
			var lock = false;
			$('#btnLoadMore').html('加载更多...');
			$('#btnLoadMore').unbind("click").click(function () {
				if (p == -1)
					return true;
				if (lock)
					return false;
				lock = true;
				p++;
				var $this = $(this);
				$this.html('正在努力加载中...');
				$.get(getPath() + '&page=' + p + "&more=1", function (result) {
					if (result.replace(/(^\s*)|(\s*$)/g, "").length == 0) {
						p = -1;
						$this.html('没有更多啦~');
					}
					else {
						var $result = $(result);
						BindArticleListEvent($result);
						$('#tbPointerArticleList').append($result);
						lock = false;
						$this.html('加载更多...');
					}
					$("a[rel=tooltip]").tooltip();
				});
			});
			BindArticleListEvent($('#tbPointerArticleList'));
		};
		//载入分组文章视图
		var reloadTagView = function (tagId) {
			currentTagId = tagId;
			currentBizId = 0;
			showLoading();
			$('#follwedArticleMain').load('/MArticle/Subscription/?tags=' + tagId + '&viewName=_TagArticleView', function () {
				bindTagArticleView();
				hideLoading();
			});
		};
		//绑定公众号文章视图事件
		var bindBizArticleView = function () {
			function BindDateArticleListEvent($html) {
				$html.find("span[rel=tooltip]").tooltip();
				$html.find('a.storeArticle').xiguauc_collection();
			}
			function BindDateArtilceListMore() {
				//设置翻页
				var p = 1;
				var lock = false;
				$('#divDateArticleList a.btnLoadMore').html('加载更多历史文章...');
				$('#divDateArticleList a.btnLoadMore').click(function () {
					if (p == -1)
						return true;
					if (lock)
						return false;
					lock = true;
					p++;
					var $this = $(this);
					$this.html('正在努力加载中...');
					$.get('/MBiz/GetMBizHistory/' + $(this).attr('data-key') + '/' + currentBizId + '/' + p, function (result) {
						if (result.replace(/(^\s*)|(\s*$)/g, "").length == 0) {
							p = -1;
							$this.html('没有更多啦~');
						}
						else {
							var $result = $(result);
							BindArticleListEvent($result);
							$('#articleHistoryListBody').append($result);
							lock = false;
							$this.html('加载更多历史文章...');
						}
					});
				});
			}
			BindDateArtilceListMore();
			BindDateArticleListEvent($('#divDateArticleList'));
			$('#follwedArticleMain ul.nav a').click(function () {
				$('#follwedArticleMain div.tabArticle').hide();
				$('#' + $(this).attr('data-target')).show();

				//按传播指数排序
				if ($(this).attr('data-target') == 'divPointerArticleList') {
					if ($('#divPointerArticleList').attr('data-loaded') != '1') {
						$('#divPointerArticleList').load('/MArticle/Subscription/?viewName=_TagArticleView&bizId=' + currentBizId, function () {
							bindTagArticleView();
						});
					}
				}
			});

			$('a.refreshData').click(function () {
				var bizId = $(this).attr('data-id');
				var self = $(this);

				$.getJSON('/MyMBiz/UpdateBizData/?bizId=' + bizId, function (result) {
					if (result.Status) {
						self.hide();
						$('#bizListFoot', self.parent().parent()).append('<a class="btn btn-default btn-xs" ><i class="fa fa-refresh fa-spin"></i> 正在为您更新数据</a>');
					}
					else {
						$.smallBox({
							title: "刷新失败",
							content: "<i class='fa fa-clock-o'></i> <i>" + result.Message + "</i>",
							color: "#C46A69",
							iconSmall: "fa fa-times fa-2x fadeInRight animated",
							timeout: 3000
						});
					}
				});
			});
			$('a.userDisabled').click(function () {
				$.SmartMessageBox({
					title: "提示",
					content: "只有高级版用户才可主动更新公众号数据",
					buttons: '[确认]'
				}, function (ButtonPressed) {

				});
			});
		};
		//载入公众号文章视图
		var reloadBizView = function(bizId,key) {
			currentBizId = bizId;
			currentTagId = 0;
			showLoading();
			$('#follwedArticleMain').load('/MArticle/BizArticleView/'+key+"/"+bizId, function () {
				bindBizArticleView();
				hideLoading();
			});
		};
		//初始化分组过滤列表
		$('#ulMyTagList div.itemBtn>a').click(function () {
			var $li = $(this).parents('li');
			$('#ulMyTagList div.itemBtn>a').removeClass('current');
			$('ul.itemList a').removeClass('current');
			$(this).addClass('current');
			reloadTagView($li.attr('data-tagId'));
		});
		$('#ulMyTagList div.itemBtn span.collapseIcon').click(function () {
			var $li = $(this).parents('li');
			if ($li.hasClass('expanded')) {
				$li.removeClass('expanded');
			}
			else {
				if ($li.attr('data-loaded') != '1') {
					$li.attr('data-loaded', '1');
					var url = '/MArticle/GetBizListByTag/?tagId=' + $li.attr('data-tagId');
					$li.find('ul.itemList').load(url, function () {
						bindBizFilterEvent($(this));
					});
				}
				$li.addClass('expanded');
			}
		});
		//默认事件执行
		bindTagArticleView();
		bindBizFilterEvent($('ul.itemList'));
		if (localStorage && !localStorage.getItem("MyTagList_FirstTag")) {
			$('#ulMyTagList li:eq(0) span.collapseIcon').click();
			if (localStorage) {
				localStorage.setItem("MyTagList_FirstTag", "1");
			}
		}

		//加载爆文列表
		function loadArticleList() {
			showLoading();
			var url = getPath();

			$('#tbPointerArticleList').load(url, function () {
				BindArticleListEvent($(this));
				$(this).find("a[rel=tooltip]").tooltip();
				hideLoading();
				if ($(this).html() == '') {
					$('#noResult').show();
					$('div.mpArticleList').hide();
				}
				else {
					$('#noResult').hide();
					$('div.mpArticleList').show();
				}
			});
		}
		function getPath() {
			//时间间隔
			var dayInterval = $('#recentlyDate a.current').attr('data-id');
			//排序
			var orderBy = $('#articleHeader a.current').attr('data-id');

			//头条过滤
			var topLine = $('#filterOnlyTopArticle').is(":checked");

			var url = '/MArticle/Subscription/?viewName=_MyArticleExplore&dayInterval=' + dayInterval;
			if (currentTagId > 0)
				url += '&tags=' + currentTagId;
			if (currentBizId > 0)
				url += '&bizId=' + currentBizId;
			url += '&orderBy=' + orderBy + "&onlyTopLine=" + (topLine ? "true" : "");
			return url;
		}

		$("#remoteModal").on("hidden", function () {
			$(this).removeData("modal");
			$("#remoteModal div.modal-content").html('');
		});
		$("#remoteModal").on("hidden.bs.modal", function () {
			$(this).removeData("bs.modal");
			$("#remoteModal div.modal-content").html('');
		});
	});
</script>