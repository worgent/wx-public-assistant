	loadScript(__uri("/xigua/js/xiguaji/xiguauc.js"), function () {
		pageSetUp();
		$(window).scroll(function () {
			if ($(window).scrollTop() >= 100) {
				$(".follwedArticleSide").addClass("fixedSide");
			} else {
				$(".follwedArticleSide").removeClass("fixedSide");
			}
		});
		$('#btnSearchMyBiz').click(function () {
			var keyword = $('#txtSearchMyBiz').val();
			if (keyword == '') {
				return;
			}
			$('#ulSearchMyBiz').show();
			$('#ulMyTagList').hide();
			var url = '/MArticle/SearchMyBiz/?bizName=' + encodeURIComponent(keyword);
			$('#ulSearchMyBiz ul.itemList').load(url, function () {
				bindBizFilterEvent($(this));
			});
		});
		$('#txtSearchMyBiz').keyup(function (e) {
			if (e.keyCode == 13) {
				$('#btnSearchMyBiz').click();
			}
		});
		$('#closeSearchMyBiz').click(function () {
			$('#ulSearchMyBiz').hide();
			$('#ulMyTagList').show();
		});

		//分组滚动固定标题栏
		$('.itemScroll').height($(window).height() - 210);
		$('.itemScroll').scroll(function () {
			var $container = $('.itemScroll');
			var targetHeight = 0;
			var flag = false;
			var height;
			$('.itemGroup').each(function (i) {
				targetHeight += $(this).outerHeight() + $(this).next('.itemList').outerHeight();
				if (flag == false && targetHeight >= $container.scrollTop()) {
					flag = true;
					height = targetHeight;
					$('.itemGroup').removeClass('fixed').css('top', '0');
					$(this).addClass('fixed').css('top', $container.scrollTop() + 'px');
				}
			});
		});

		//绑定公众号过滤事件
		var bindBizFilterEvent = function ($html) {
			$html.find('a').click(function () {
				if (!$(this).hasClass('current')) {
					$('ul.itemList a').removeClass('current');
					$('#ulMyTagList div.itemBtn>a').removeClass('current');
					$(this).addClass('current');
					reloadBizView($(this).attr('data-bizId'),$(this).attr('data-id'));
				}
			});
		};

		//设置列表事件
		var BindArticleListEvent = function ($html) {
			$html.find('a.articlecollect').xiguauc_collection();
			$html.find("[rel=tooltip]").tooltip();
