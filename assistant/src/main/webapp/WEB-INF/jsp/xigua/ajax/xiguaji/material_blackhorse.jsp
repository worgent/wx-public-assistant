<script type="text/javascript">
	pageSetUp();
	var noResult = false;
	var IsAdvancedUser = false;
	var IsRegularUser = false;
	var SelfTagId = '128';

	function init() {
		//标签设置
		$('.popularTab li a').click(function() {
			$('.popularTab li.current').removeClass('current');
			$(this).parent().addClass('current');
			triggerArticleFilter();

			if(IsAdvancedUser || (IsRegularUser && SelfTagId == $(this).attr('data-tagid'))) {
				$('#loadMore').css('display', '');
				$('#noLoadMore').css('display', 'none');
			} else {
				$('#loadMore').css('display', 'none');
				$('#noLoadMore').css('display', '');
			}
		});
		if(IsAdvancedUser || (IsRegularUser && SelfTagId == $('.popularTab li.current a').attr('data-tagid'))) {
			$('#loadMore').css('display', '');
			$('#noLoadMore').css('display', 'none');
		} else {
			$('#loadMore').css('display', 'none');
			$('#noLoadMore').css('display', '');
		}

		//设置时间
		$('#btnPopularDate').click(function() {
			$('#popularDatePool').toggleClass('open');
		});
		$('#popularDateMenu li a').click(function() {
			$('#popularDatePool').removeClass('open');
			$('#popularDateNote').attr('data-datecode', $(this).attr('data-datecode'));
			$('#popularDateNote').attr('data-index', $(this).attr('data-index'));
			$('#popularDateNote').text($(this).text());
			triggerArticleFilter();
		});
		$('#btnPopularDatePre').click(function() {
			var prev = $('#popularDateMenu li a[data-index=' + (parseInt($('#popularDateNote').attr('data-index')) +1) + ']');
			if(prev.length>0) {
				$('#popularDateNote').attr('data-datecode', $(prev).attr('data-datecode'));
				$('#popularDateNote').attr('data-index', $(prev).attr('data-index'));
				$('#popularDateNote').text($(prev).text());
				triggerArticleFilter();
			}
		});
		$('#btnPopularDateNext').click(function() {
			var next = $('#popularDateMenu li a[data-index=' + (parseInt($('#popularDateNote').attr('data-index')) -1) + ']');
			if(next.length>0) {
				$('#popularDateNote').attr('data-datecode', $(next).attr('data-datecode'));
				$('#popularDateNote').attr('data-index', $(next).attr('data-index'));
				$('#popularDateNote').text($(next).text());
				triggerArticleFilter();
			}
		});

		//设置翻页
		BindMore();

		//设置列表事件
		BindArticleListEvent($('#popularArticleListBody'));


		//设置可见度
		setNoResult(noResult);

	}
	init();

	function setNoResult(noResult) {
		if(noResult) {
			$('#noResultInfo').css("display", "");
			$('#popularArticleList').css("display", "none");
		} else {
			$('#noResultInfo').css("display", "none");
			$('#popularArticleList').css("display", "");
		}
	}

	function BindMore() {
		//设置翻页
		var p = 1;
		var lock = false;
		$('#btnLoadMore').html('加载更多...');
		$('#btnLoadMore').unbind("click").click(function () {
			if (p == -1)
				return true;
			if (lock)
				return false;
			lock = true;
			p++;
			var $this = $(this);
			$this.html('正在努力加载中...');
			$.get(getPath() + '&page=' + p, function (result) {
				if (result.replace(/(^\s*)|(\s*$)/g, "").length == 0) {
					p = -1;
					$this.html('没有更多啦~');
				}
				else {
					var $result = $(result);
					BindArticleListEvent($result);
					$('#popularArticleListBody').append($result);
					lock = false;
					$this.html('加载更多...');
				}
			});
		});
	}

